@model FinalYear.Models.summary

@foreach (var entry in ViewBag.EvaluationResults)
{
    <div class="card text-white bg-danger mb-3" style="max-width: 40rem;">
        <div class="card-header">@entry.Name</div>
        <div class="card-body">
            <p>
                @entry.Values
            </p>
        </div>
    </div>
}
